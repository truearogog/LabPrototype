<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:settings-listings="using:LabPrototype.Views.Components.SettingsListings"
             x:Class="LabPrototype.Views.Components.ModelSettings">
	<ExperimentalAcrylicBorder Grid.Column="0" Classes="b60">

		<TabControl>
			<TabItem Header="Color Schemes">
				<settings-listings:ColorSchemeSettingsListing DataContext="{Binding ColorSchemeSettingsListingViewModel}" />
			</TabItem>
			<TabItem Header="Meters">
				<settings-listings:MeterSettingsListing DataContext="{Binding MeterSettingsListingViewModel}" />
			</TabItem>
			<TabItem Header="Meter Types" IsEnabled="False">
				<settings-listings:MeterTypeSettingsListing DataContext="{Binding MeterTypeSettingsListingViewModel} " />
			</TabItem>
			<TabItem Header="Measurement Types" IsEnabled="False">
				<settings-listings:MeasurementTypeSettingsListing DataContext="{Binding MeasurementTypeSettingsListingViewModel}" />
			</TabItem>
		</TabControl>
	</ExperimentalAcrylicBorder>
</UserControl>
